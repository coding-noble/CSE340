<% if (title) { %>
    <div id="archive">
        <h1>
            <%= title %>
        </h1>
        <%- messages() %>
        <% if (errors) { %>
            <ul class="notice">
                <% errors.array().forEach(error=> { %>
                    <li>
                        <%= error.msg %>
                    </li>
                <% }) %>
            </ul>
        <% } %>
        
        <ul>
            <li><a href="/message/inbox/<%= locals.accountData.account_id %>">Return to Inbox</a></li>
        </ul>

        <%- archivedInbox %>
    </div>
<% } else { %>
    <% res.redirect('/') %>
<% } %>