<% if (title) { %>
<div id="loggedin">
    <h1>
        <%= title %>
    </h1>
    <%- messages() %>
    <% if (errors) { %>
        <ul class="notice">
        <% errors.array().forEach(error=> { %>
            <li>
                <%= error.msg %>
            </li>
            <% }) %>
        </ul>
    <% } %>
    <h1>You're logged in</h1>
</div>
<% } else { %>
    <% res.redirect('/') %>
<% } %>