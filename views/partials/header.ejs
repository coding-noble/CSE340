<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (locals.loggedin) { %>
      <a href="/account/management">Welcome <%= locals.accountData.account_firstname %> </a>
      <a title="Click to logout" href="/">Logout</a>
      <p><a href="">Update Account Information</a></p>
      <% if (locals.accountData.account_type == "Employee" || locals.accountData.account_type == "Admin") { %>
        <h3>Inventory Management</h3>
        <a href="/account/management">Go To Account Management</a>
      <% } %>
      <% } else { %>
        <a title="Click to login" href="/account/login">My Account</a>
      <% } %>
  </div>
</header>